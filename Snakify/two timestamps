hour1 = int(input())
minute1 = int(input())
second1 = int(input())
timestamp1 = int((hour1 * 3600) + (minute1 * 60) + second1)
hour2 = int(input())
minute2 = int(input())
second2 = int(input())
timestamp2 = int((hour2 * 3600) + (minute2 * 60) + second2)
Moment = int(timestamp2 - timestamp1)
print(Moment)
